    "html editor",
    "web editor",
    "typescript editor"
  ],
  "dependencies": {
    "@tanbo/di": "^1.1.3",
    "@tanbo/stream": "^1.1.5",
    "immer": "^9.0.12"
  },
  "author": {
    "name": "Tanbo",
    "email": "tanbohb@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textbus/textbus.git"
